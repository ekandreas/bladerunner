{
    "name": "ekandreas/bladerunner",
    "type": "wordpress-plugin",
    "description": "WordPress Blade L5 template engine",
    "keywords": ["laravel", "blade", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Ek",
            "email": "andreas@elseif.se"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/config": "^5.0",
        "illuminate/view": "^5.0",
        "symfony/yaml": "^3.0",
        "brain/hierarchy": "^2.0"
    },
    "require-dev": {
        "frozzare/wp-test-suite": "*"
    },
    "autoload": {
        "psr-4": {
            "Bladerunner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bladerunner\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.7-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}