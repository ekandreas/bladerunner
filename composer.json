{
    "name": "ekandreas/bladerunner",
    "type": "wordpress-plugin",
    "description": "WordPress Blade L5/6/7/8 template engine",
    "keywords": ["laravel", "blade", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Ek",
            "email": "andreas@elseif.se"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "illuminate/config": "^5 || ^6 || ^7 || ^8",
        "illuminate/view": "^5 || ^6 || ^7 || ^8",
        "symfony/yaml": "^3",
        "brain/hierarchy": "^2"
    },
    "require-dev": {
        "frozzare/wp-test-suite": "*"
    },
    "autoload": {
        "psr-4": {
            "Bladerunner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bladerunner\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
